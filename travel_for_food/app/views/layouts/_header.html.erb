<header>
<% if !logged_in? %>
  <%= link_to("Log In", login_path) %>
  <%= link_to("Sign Up", signup_path) %>
<% else %>
  <%= link_to("triptracker", user_path(current_user), :class => 'logo') %>
  <%= link_to("My Trips", user_trips_path(current_user)) %>
  <%= link_to("Restaurants", restaurants_path) %>
  <%= link_to("Travelers", users_path) %>
  <%= link_to("Log Out", logout_path) %>
<% end %>
</header>
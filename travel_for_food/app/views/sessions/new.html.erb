<div class="start-pages signup-login">
<h1>Travel for Food <br> Log in</h1>
<p><i><%= @error %></p></i>
<%= form_for :user do |f| %>
    <%=f.label :username%>
    <%= f.text_field :username%><br><br>
     <%=f.label :password%>
    <%= f.password_field :password%><br><br>
    <%= f.submit "Log in"%><br>
  <% end %>

  <br><br><h2><%= link_to "Log In With Github", '/auth/github' %></h2>